<h1>New</h1>
<br><br>
<div class="panel-heading"><h3>Sign in 👇🏽</h3></div>
<br><br>

<%=form_tag url_for(action:'create'), method:"post" do %>

  <%=label_tag'first name :'%><br>
  <%= text_field_tag 'first_name'%><br><br>

  <%=label_tag'last name :'%><br>
  <%= text_field_tag 'last_name'%><br><br>

  <%=label_tag'city :'%><br>
  <%= text_field_tag 'city'%><br><br>

  <%=label_tag'password :'%><br>
  <%= text_field_tag 'password'%><br><br>

  <%=label_tag'email :'%><br>
  <%= text_field_tag 'email'%><br><br>

  <%= submit_tag "Send" %>
<%end%>
<br><br>
